package com.hyrtutorials.telugu;

public class Cat 
{
	public static void main(String[] args) 
	{
		byte b = 50;
		short s = 250;
		int i = 2145653;
		long l = 99663717475L;
		float f = 3.14f;
		double d = 88.2456;
		boolean b1 = true;
		boolean b2 = false;
		char c = 'A';
		
		System.out.println(Integer.MIN_VALUE);
		System.out.println(Integer.MAX_VALUE);
	}
}
